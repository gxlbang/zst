        if (typeof (M) == 'undefined' || !M) {
            window.M = {};
        }
    </script>
</head>

<body id="body" class="s">
    <div class="container">
        <div class="page-nub">
            <span class="page-tit">图品展示</span>
            <div class="page-page"> <em id="slide-nub" class="fz18">2</em> <em class="nub-bg">/</em> <em id="slide-sum" class="fz12">3</em> </div>
            <div class="return-allimg"><a href="#" onclick="window.parent.layer.closeAll()" class="a-morere"><img src="/Content/UiImages/re-all.png" alt=""></a></div>
        </div>
        <!-- 焦点图片 -->
        <div class="scroll-imgs" style="top: 50%; margin-top: -187.5px;">
            <div id="slide" class="slide" style="height: 375px;">
                <ul id="imagelist" data-slide-ul="firstUl" style="width: 1875px; height: 375px;">
                    @foreach (var item in Model)
                    {
                        <li data-ul-child="child" onClick="window.history.go(-1)" style="transition: all 300ms ease; height: 375px; width: 375px; transform: translate3d(-375px, 0px, 0px);">
                            <div class="PinchZoom">
                                <img alt="@item.ImageTitle" src="@item.ImageUrl" onerror="imgError();"
                                     class="J_ping" report-eventid="MProductdetail_BigPicClick" report-pageparam="3907358">
                            </div>
                        </li>
                    }

                </ul>
                <div class="tittup" id="tittup" style="transform: translate3d(4.3333px, 0px, 0px);">
                    <div class="arrow-box"> <em class="arrow"></em> <em class="arrow2"></em> </div>
                    <span class="inner">没有啦</span>
                </div>
            </div>
        </div>
    </div>
    <input type="hidden" id="picInitNum" value="1">
    <script type="text/javascript" src="~/Content/Scripts/uijs/Picture display/zepto.min.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/uijs/Picture display/pic.js?s=0"></script>
    <script src="~/Content/Scripts/uijs/Picture display/underscore.js" type="text/javascript"></script>
    <script src="~/Content/Scripts/uijs/Picture display/pinchzoom.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            $('.PinchZoom').each(function () {
                new RTP.PinchZoom($(this), {});
            });
        })
    </script>
</body>
</html>
