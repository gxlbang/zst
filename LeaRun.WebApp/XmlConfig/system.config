  <!-- 软件许可证编号 -->
  <add key="Licence" value="NO:2013101712345678" />
  <!-- ================== 4：系统邮件错误报告反馈相关 ================== -->
  <!-- 软件的错误报告发给谁 -->
  <add key="ErrorReportTo" value="393319165@qq.com,393319165@qq.com" />
  <!-- 发出错误邮箱的地址 -->
