@{
    ViewBag.Title = "抽蛋记录 抽蛋记录》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/ProductModule/Fx_MoveOrderRecord/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>编号：</th>
            <td class='formValue'>
                <label id="Number" />
            </td>
            <th class='formTitle'>关联编号 订单编号：</th>
            <td class='formValue'>
                <label id="OrderNumber" />
            </td>
            <th class='formTitle'>关联编号 原用户编号：</th>
            <td class='formValue'>
                <label id="UserNumber" />
            </td>
            <th class='formTitle'>原用户登录名：</th>
            <td class='formValue'>
                <label id="UserAccount" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>原用户名：</th>
            <td class='formValue'>
                <label id="UserName" />
            </td>
            <th class='formTitle'>关联编号 拿到订单的用户：</th>
            <td class='formValue'>
                <label id="MyUser" />
            </td>
            <th class='formTitle'>拿到订单的用户姓名：</th>
            <td class='formValue'>
                <label id="MyUserName" />
            </td>
            <th class='formTitle'>拿到订单的用户登录名：</th>
            <td class='formValue'>
                <label id="MyAccount" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>拿单时间：</th>
            <td class='formValue'>
                <label id="CreateTime" />
            </td>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
    </table>
</form>
