@{
    ViewBag.Title = "Fx_UserPVCount》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/ProductModule/Fx_UserPVCount/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>Number：</th>
            <td class='formValue'>
                <label id="Number" />
            </td>
            <th class='formTitle'>UNumber：</th>
            <td class='formValue'>
                <label id="UNumber" />
            </td>
            <th class='formTitle'>UserName：</th>
            <td class='formValue'>
                <label id="UserName" />
            </td>
            <th class='formTitle'>RealName：</th>
            <td class='formValue'>
                <label id="RealName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>IP：</th>
            <td class='formValue'>
                <label id="IP" />
            </td>
            <th class='formTitle'>Browser：</th>
            <td class='formValue'>
                <label id="Browser" />
            </td>
            <th class='formTitle'>Sign：</th>
            <td class='formValue'>
                <label id="Sign" />
            </td>
            <th class='formTitle'>CreateTime：</th>
            <td class='formValue'>
                <label id="CreateTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>OpenId：</th>
            <td class='formValue'>
                <label id="OpenId" />
            </td>
            <th class='formTitle'>Remark：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
    </table>
</form>
