@{
    ViewBag.Title = "Fx_WebAdv》表单页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    var KeyValue = GetQuery('KeyValue');//主键
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/WebModule/Fx_WebAdv/SetForm", { KeyValue: KeyValue }, function (data) {
                SetWebControls(data);
            });
        }
    }
    //保存事件
    function AcceptClick() {
        if (!CheckDataValid('#form1')) {
            return false;
        }
        var postData = GetWebControls("#form1");
        AjaxJson("/WebModule/Fx_WebAdv/SubmitForm?KeyValue=" + KeyValue, postData, function (data) {
            tipDialog(data.Message, 3, data.Code);
            top.frames[tabiframeId()].windowload();
            closeDialog();
        });
    }
</script>
<form id="form1" style="margin: 1px">
    <div id="message" style="display: none"></div>
    <table class="form">
        <tr>
            <th class='formTitle'>Number：</th>
            <td class='formValue'>
                <input id="Number" maxlength=50 type="text" class="txt"   />
            </td>
            <th class='formTitle'>Title：</th>
            <td class='formValue'>
                <input id="Title" maxlength=128 type="text" class="txt"   />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>AdvDes：</th>
            <td class='formValue'>
                <input id="AdvDes" maxlength=256 type="text" class="txt"   />
            </td>
            <th class='formTitle'>AdvImg：</th>
            <td class='formValue'>
                <input id="AdvImg" maxlength=256 type="text" class="txt"   />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>AdvLink：</th>
            <td class='formValue'>
                <input id="AdvLink" maxlength=256 type="text" class="txt"   />
            </td>
            <th class='formTitle'>CreateTime：</th>
            <td class='formValue'>
                <input id="CreateTime" maxlength=23 type="text" class="txt"   />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>Status：</th>
            <td class='formValue'>
                <input id="Status" maxlength=10 type="text" class="txt"   />
            </td>
            <th class='formTitle'>IsDel：</th>
            <td class='formValue'>
                <input id="IsDel" maxlength=5 type="text" class="txt"   />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>JsFile：</th>
            <td class='formValue'>
                <input id="JsFile" maxlength=128 type="text" class="txt"   />
            </td>
            <th class='formTitle'>Width：</th>
            <td class='formValue'>
                <input id="Width" maxlength=5 type="text" class="txt"   />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>Height：</th>
            <td class='formValue'>
                <input id="Height" maxlength=5 type="text" class="txt"   />
            </td>
            <th class='formTitle'>Remark：</th>
            <td class='formValue'>
                <input id="Remark" maxlength=128 type="text" class="txt"   />
            </td>
        </tr>
        <tr>
        </tr>
    </table>
</form>
