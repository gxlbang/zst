@{
    ViewBag.Title = "合伙人》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/HouseModule/Ho_PartnerUser/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebLable(data);
                $("#Remarkstr").val(data.Remark);
                $("#pimg1").attr("src", (!IsNullOrEmpty(data.CodeImg1) ? "/Content/Images/noimg.gif" : data.CodeImg1));
                $("#pimg2").attr("src", (!IsNullOrEmpty(data.CodeImg2) ? "/Content/Images/noimg.gif" : data.CodeImg2));
                $("#CreatTime").text(formatDate(data.CreatTime, "yyyy-MM-dd hh:mm:ss"));
                $("#SureTime").text(formatDate(data.SureTime, "yyyy-MM-dd hh:mm:ss"));
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>编号：</th>
            <td class='formValue'>
                <label id="InnerCode" />
            </td>
            <th width="10"></th>
            <td class='formValue' rowspan="5">
                <img src="~/Content/Images/noimg.gif" id="pimg1" width="200" height="150" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>姓名：</th>
            <td class='formValue'>
                <label id="Name" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>身份证号：</th>
            <td class='formValue'>
                <label id="CardCode" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>手机号：</th>
            <td class='formValue'>
                <label id="Mobile" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>微信帐号：</th>
            <td class='formValue'>
                <label id="WeiXin" />
            </td>
        </tr>

        <tr>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreatTime" />
            </td>
            <th width="10"></th>
            <td class='formValue' rowspan="5">
                <img src="~/Content/Images/noimg.gif" id="pimg2" width="200" height="150" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>审核时间：</th>
            <td class='formValue'>
                <label id="SureTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>审核人：</th>
            <td class='formValue'>
                <label id="SureUser" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>上级名称：</th>
            <td class='formValue'>
                <label id="ParentName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle' style="color:red;">状态：</th>
            <td class='formValue'>
                <label id="StatusStr" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>备注：</th>
            <td class='formValue' colspan="3">
                <textarea id="Remarkstr" name="Remarkstr" maxlength="600" class="txtArea" rows="8"></textarea>
            </td>
        </tr>
    </table>
</form>
