@{
    ViewBag.Title = "Ho_CityInfo》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/HouseModule/Ho_CityInfo/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>Number：</th>
            <td class='formValue'>
                <label id="Number" />
            </td>
            <th class='formTitle'>ProvinceNumber：</th>
            <td class='formValue'>
                <label id="ProvinceNumber" />
            </td>
            <th class='formTitle'>Province：</th>
            <td class='formValue'>
                <label id="Province" />
            </td>
            <th class='formTitle'>CityNumber：</th>
            <td class='formValue'>
                <label id="CityNumber" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>City：</th>
            <td class='formValue'>
                <label id="City" />
            </td>
            <th class='formTitle'>Orders：</th>
            <td class='formValue'>
                <label id="Orders" />
            </td>
            <th class='formTitle'>Remark：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
    </table>
</form>
