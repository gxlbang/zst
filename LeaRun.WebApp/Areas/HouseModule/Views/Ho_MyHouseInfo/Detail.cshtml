@{
    ViewBag.Title = "Ho_MyHouseInfo》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/HouseModule/Ho_MyHouseInfo/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>Number：</th>
            <td class='formValue'>
                <label id="Number" />
            </td>
            <th class='formTitle'>UNumber：</th>
            <td class='formValue'>
                <label id="UNumber" />
            </td>
            <th class='formTitle'>UName：</th>
            <td class='formValue'>
                <label id="UName" />
            </td>
            <th class='formTitle'>HNumber：</th>
            <td class='formValue'>
                <label id="HNumber" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>HName：</th>
            <td class='formValue'>
                <label id="HName" />
            </td>
            <th class='formTitle'>Himg：</th>
