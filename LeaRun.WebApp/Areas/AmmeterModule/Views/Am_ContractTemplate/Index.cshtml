                { label: 'HallNum', name: 'HallNum', index: 'HallNum', width: 0, align: 'left',sortable: true  },
                { label: 'KitchenNum', name: 'KitchenNum', index: 'KitchenNum', width: 0, align: 'left',sortable: true  },
                { label: 'ToiletNum', name: 'ToiletNum', index: 'ToiletNum', width: 0, align: 'left',sortable: true  },
