@{
    ViewBag.Title = "Am_Ammeter";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
<script type="text/javascript">
    $(function () {
        GetGrid();
    })
    //加载表格
    function GetGrid() {
        $("#gridTable").jqGrid({
        url: "@Url.Content("~/AmmeterModule/Am_Ammeter/GridPageJson")",
        datatype: "json",
        height: $(window).height() - 149,
        autowidth: true,
        colModel: [
                { label: 'Number', name: 'Number', index: 'Number', width: 0, align: 'left',sortable: true  },
                { label: 'AM_Code', name: 'AM_Code', index: 'AM_Code', width: 0, align: 'left',sortable: true  },
                { label: 'AmmeterType_Number', name: 'AmmeterType_Number', index: 'AmmeterType_Number', width: 0, align: 'left',sortable: true  },
                { label: 'AmmeterType_Name', name: 'AmmeterType_Name', index: 'AmmeterType_Name', width: 0, align: 'left',sortable: true  },
                { label: 'AmmeterMoney_Number', name: 'AmmeterMoney_Number', index: 'AmmeterMoney_Number', width: 0, align: 'left',sortable: true  },
                { label: 'AmmeterMoney_Name', name: 'AmmeterMoney_Name', index: 'AmmeterMoney_Name', width: 0, align: 'left',sortable: true  },
                { label: 'Collector_Number', name: 'Collector_Number', index: 'Collector_Number', width: 0, align: 'left',sortable: true  },
                { label: 'Collector_Code', name: 'Collector_Code', index: 'Collector_Code', width: 0, align: 'left',sortable: true  },
                { label: 'U_Number', name: 'U_Number', index: 'U_Number', width: 0, align: 'left',sortable: true  },
                { label: 'UserName', name: 'UserName', index: 'UserName', width: 0, align: 'left',sortable: true  },
                { label: 'U_Name', name: 'U_Name', index: 'U_Name', width: 0, align: 'left',sortable: true  },
                { label: 'Province', name: 'Province', index: 'Province', width: 0, align: 'left',sortable: true  },
                { label: 'City', name: 'City', index: 'City', width: 0, align: 'left',sortable: true  },
                { label: 'County', name: 'County', index: 'County', width: 0, align: 'left',sortable: true  },
                { label: 'Cell', name: 'Cell', index: 'Cell', width: 0, align: 'left',sortable: true  },
                { label: 'Floor', name: 'Floor', index: 'Floor', width: 0, align: 'left',sortable: true  },
                { label: 'Room', name: 'Room', index: 'Room', width: 0, align: 'left',sortable: true  },
                { label: 'Address', name: 'Address', index: 'Address', width: 0, align: 'left',sortable: true  },
                { label: 'Status', name: 'Status', index: 'Status', width: 0, align: 'left',sortable: true  },
                { label: 'StatusStr', name: 'StatusStr', index: 'StatusStr', width: 0, align: 'left',sortable: true  },
                { label: 'CreateTime', name: 'CreateTime', index: 'CreateTime', width: 0, align: 'left',sortable: true  },
                { label: 'UpdateTime', name: 'UpdateTime', index: 'UpdateTime', width: 0, align: 'left',sortable: true  },
                { label: 'HGQBB', name: 'HGQBB', index: 'HGQBB', width: 0, align: 'left',sortable: true  },
                { label: 'FirstAlarm', name: 'FirstAlarm', index: 'FirstAlarm', width: 0, align: 'left',sortable: true  },
                { label: 'SencondAlarm', name: 'SencondAlarm', index: 'SencondAlarm', width: 0, align: 'left',sortable: true  },
                { label: 'Money', name: 'Money', index: 'Money', width: 0, align: 'left',sortable: true  },
                { label: 'AllMoney', name: 'AllMoney', index: 'AllMoney', width: 0, align: 'left',sortable: true  },
                { label: 'CurrPower', name: 'CurrPower', index: 'CurrPower', width: 0, align: 'left',sortable: true  },
                { label: 'CP_Time', name: 'CP_Time', index: 'CP_Time', width: 0, align: 'left',sortable: true  },
                { label: 'CurrMoney', name: 'CurrMoney', index: 'CurrMoney', width: 0, align: 'left',sortable: true  },
                { label: 'CM_Time', name: 'CM_Time', index: 'CM_Time', width: 0, align: 'left',sortable: true  },
                { label: 'UY_Number', name: 'UY_Number', index: 'UY_Number', width: 0, align: 'left',sortable: true  },
                { label: 'UY_UserName', name: 'UY_UserName', index: 'UY_UserName', width: 0, align: 'left',sortable: true  },
                { label: 'UY_Name', name: 'UY_Name', index: 'UY_Name', width: 0, align: 'left',sortable: true  },
                { label: 'Remark', name: 'Remark', index: 'Remark', width: 0, align: 'left',sortable: true  },
        ],
        pager: "#gridPager",
        sortname: '',
        sortorder: 'desc',
        rownumbers: true,
        shrinkToFit: false,
        gridview: true,
    });
}
    //新增
    function btn_add() {
        var url = "/AmmeterModule/Am_Ammeter/Form"
        openDialog(url, "Form", "新增Am_Ammeter", 770, 395, function (iframe) {
            top.frames[iframe].AcceptClick()
        });
    }
    //编辑
    function btn_edit() {
        var KeyValue = GetJqGridRowValue("#gridTable", "AmmeterMoney_Number");
        if (IsChecked(KeyValue)) {
            var url = "/AmmeterModule/Am_Ammeter/Form?KeyValue=" + KeyValue;
            openDialog(url, "Form", "编辑Am_Ammeter", 770, 395, function (iframe) {
                top.frames[iframe].AcceptClick();
            });
        }
    }
    //删除
    function btn_delete() {
        var KeyValue = GetJqGridRowValue("#gridTable", "AmmeterMoney_Number");
        if (IsDelData(KeyValue)) {
            var delparm = 'KeyValue=' + KeyValue;
            delConfig('/AmmeterModule/Am_Ammeter/Delete', delparm, KeyValue.split(",").length);
        }
    }
    //明细
    function btn_detail() {
        var KeyValue = GetJqGridRowValue("#gridTable", "AmmeterMoney_Number");
        if (IsChecked(KeyValue)) {
            var url = "/AmmeterModule/Am_Ammeter/Detail?KeyValue=" + KeyValue;
            Dialog(url, "Detail", "Am_Ammeter明细", 820, 500, function (iframe) {
                top.frames[iframe].AcceptClick();
            });
        }
    }
    //刷新
    function windowload() {
        $("#gridTable").trigger("reloadGrid"); //重新载入
        
    }
</script>
<div id="layout" class="layout">
    <!--左边-->
    <div class="layoutPanel layout-west">
        <div class="btnbartitle">
            <div>
                组织结构
            </div>
        </div>
        <div class="ScrollBar" id="ItemsTree"></div>
    </div>
    <!--中间-->
    <div class="layoutPanel layout-center">
        <div class="btnbartitle">
            <div>
                用户列表 <span id="CenterTitle"></span>
            </div>
        </div>
        <!--工具栏-->
        <div class="tools_bar" style="border-top: none; margin-bottom: 0px;">
            <div class="PartialButton">
                @Html.Partial("_PartialButton")
            </div>
        </div>
        <!--列表-->
        <div id="grid_List">
            <div class="bottomline QueryArea" style="margin: 1px; margin-top: 0px; margin-bottom: 0px;">
                <table border="0" class="form-find" style="height: 45px;">
                    <tr>
                        <th>关键字：
                        </th>
                        <td>
                            <input id="keywords" type="text" class="txt" style="width: 200px" />
                        </td>
                        <td>
                            <input id="btnSearch" type="button" class="btnSearch" value="搜 索" onclick="btn_Search()" />
                        </td>
                    </tr>
                </table>
            </div>
            <table id="gridTable"></table>
            <div id="gridPager"></div>
        </div>
    </div>
</div>
