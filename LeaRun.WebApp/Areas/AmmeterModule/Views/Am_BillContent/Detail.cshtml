@{
    ViewBag.Title = "Am_BillContent》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/AmmeterModule/Am_BillContent/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>Number：</th>
            <td class='formValue'>
                <label id="Number" />
            </td>
            <th class='formTitle'>Bill_Number：</th>
            <td class='formValue'>
                <label id="Bill_Number" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>Bill_Code：</th>
            <td class='formValue'>
                <label id="Bill_Code" />
            </td>
            <th class='formTitle'>ChargeItem_Number：</th>
            <td class='formValue'>
                <label id="ChargeItem_Number" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>ChargeItem_Title：</th>
            <td class='formValue'>
                <label id="ChargeItem_Title" />
            </td>
            <th class='formTitle'>ChargeItem_ChargeType：</th>
            <td class='formValue'>
                <label id="ChargeItem_ChargeType" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>Money：</th>
            <td class='formValue'>
                <label id="Money" />
            </td>
            <th class='formTitle'>UMark：</th>
            <td class='formValue'>
                <label id="UMark" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>Remark：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
    </table>
</form>
