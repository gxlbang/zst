@{
    ViewBag.Title = "Am_ChargeItem》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/AmmeterModule/Am_ChargeItem/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebLable(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>收费项目：</th>
            <td class='formValue'>
                <label id="Title" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>默认金额：</th>
            <td class='formValue'>
                <label id="Money" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>收费类别：</th>
            <td class='formValue'>
                <label id="ChargeType" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <textarea id="Remark" name="Remark" maxlength="600" class="txtArea" rows="8"></textarea>
            </td>
        </tr>
        <tr></tr>
    </table>
</form>
