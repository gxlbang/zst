@{
    ViewBag.Title = "Am_RepairImage》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/AmmeterModule/Am_RepairImage/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>Number：</th>
            <td class='formValue'>
                <label id="Number" />
            </td>
            <th class='formTitle'>Repair_Number：</th>
            <td class='formValue'>
                <label id="Repair_Number" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>ImagePath：</th>
            <td class='formValue'>
                <label id="ImagePath" />
            </td>
            <th class='formTitle'>ImageMark：</th>
            <td class='formValue'>
                <label id="ImageMark" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>Remark：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
    </table>
</form>
