@{
    ViewBag.Title = "Am_UserGetMoneyToBank》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/AmmeterModule/Am_UserGetMoneyToBank/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebLable(data);
                $("#Remark").val(data.Remark);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>用户名：</th>
            <td class='formValue'>
                <label id="UserName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>姓名：</th>
            <td class='formValue'>
                <label id="U_Name" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>提现金额：</th>
            <td class='formValue'>
                <label id="Money" style="color:green;font-weight:bold;" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>提现时间：</th>
            <td class='formValue'>
                <label id="CreateTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>支付时间：</th>
            <td class='formValue'>
                <label id="PayTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>银行名称：</th>
            <td class='formValue'>
                <label id="BankName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>银行卡号：</th>
            <td class='formValue'>
                <label id="BankCode" style="color:blue;font-weight:bold;" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>开户行：</th>
            <td class='formValue'>
                <label id="BankAddress" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>手续费：</th>
            <td class='formValue'>
                <label id="BankCharge" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>实际到账：</th>
            <td class='formValue'>
                <label id="RealMoney" style="color:red;font-weight:bold;" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>状态：</th>
            <td class='formValue'>
                <label id="StatusStr" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <textarea id="Remark" name="Remark" maxlength="200" class="txtArea" rows="5"></textarea>
            </td>
        </tr>
    </table>
</form>
