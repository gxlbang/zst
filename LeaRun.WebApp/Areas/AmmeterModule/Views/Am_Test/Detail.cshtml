@{
    ViewBag.Title = "Am_Test》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/AmmeterModule/Am_Test/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebLable(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>李世勋：</th>
            <td class='formValue'>
                <label id="Testa" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>数量：</th>
            <td class='formValue'>
                <label id="Num" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreateTime" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>备注：</th>
            <td class='formValue'>
                <textarea id="Remark" name="Remark" maxlength="200" class="txtArea" rows="8"></textarea>
            </td>
        </tr>
    </table>
</form>
